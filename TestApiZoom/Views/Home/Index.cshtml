@model TestApiZoom.Models.Meetings

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("New meeting","Index","Meeting")
</p>
<table class="table">
    <tr>
        <th>
            @Html.Label("Topic:")
        </th>
        <th>
            @Html.Label("ID:")
        </th>
        <th>
            @Html.Label("Link:")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.meetings)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.topic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            <a href=@item.join_url target="_blank">Join</a>
        </td>
       
    </tr>
    }

</table>
